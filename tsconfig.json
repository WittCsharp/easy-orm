{
    "compilerOptions": {
        "noImplicitAny": false,
        "noEmitOnError": true,
        "removeComments": true,
        "sourceMap": true,
        "target": "es5",
        "outDir": "src",
        "lib": ["ES2015"],
        "declaration": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
    },
    "include": [
      "lib/**/*"
    ],
    "exclude": [
        "node_modules"
    ]
  }